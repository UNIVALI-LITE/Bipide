    <system:String x:Key="TipDescriptionButtonContinue">Continua a simulação do programa até o seu final.</system:String>

    <system:String x:Key="TipTitleButtonStop">Parar</system:String>
    <system:String x:Key="TipDescriptionButtonStop">Interrompe a simulação do programa.</system:String>

