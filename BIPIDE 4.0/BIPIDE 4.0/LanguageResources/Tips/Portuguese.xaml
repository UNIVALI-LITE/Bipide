<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="TipTitleButtonNew">Novo</system:String>
    <system:String x:Key="TipDescriptionButtonNew">Cria um novo documento.</system:String>

    <system:String x:Key="TipTitleButtonOpen">Abrir</system:String>
    <system:String x:Key="TipDescriptionButtonOpen">Abre um arquivo existente.</system:String>

    <system:String x:Key="TipTitleButtonSave">Salvar</system:String>
    <system:String x:Key="TipDescriptionButtonSave">Salvar o documento atual.</system:String>

    <system:String x:Key="TipTitleButtonSaveAs">Salvar Como</system:String>
    <system:String x:Key="TipDescriptionButtonSaveAs">Salva uma cópia do documento atual.</system:String>

    <system:String x:Key="TipTitleButtonPrint">Imprimir</system:String>
    <system:String x:Key="TipDescriptionButtonPrint">Imprime o documento atual.</system:String>

    <system:String x:Key="TipTitleButtonExamples">Exemplos</system:String>
    <system:String x:Key="TipDescriptionButtonExamples">Possibilita abrir programas de exemplo.</system:String>

    <system:String x:Key="TipTitleButtonExport">Exportar</system:String>
    <system:String x:Key="TipDescriptionButtonExport">Permite exportar o programa nos formatos: Assembly, Binário, Hexadecimal, MIF e VHDL.</system:String>

    <system:String x:Key="TipTitleButtonCompile">Compilar</system:String>
    <system:String x:Key="TipDescriptionButtonCompile">Compila o programa.</system:String>

