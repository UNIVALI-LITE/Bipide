    <system:String x:Key="TipTitleButtonRedo">Refazer</system:String>
    <system:String x:Key="TipDescriptionButtonRedo">Refaz a última ação desfeita.</system:String>

    <system:String x:Key="TipTitleButtonExit">Sair</system:String>
    <system:String x:Key="TipDescriptionButtonExit">Sai do sistema.</system:String>   

    <system:String x:Key="TipDescriptionRadioButtonBipI">Seleciona o processador BIP I para ser utilizado na simulação.</system:String>

    <system:String x:Key="TipDescriptionRadioButtonBipII">Seleciona o processador BIP II para ser utilizado na simulação.</system:String>

    <system:String x:Key="TipDescriptionRadioButtonBipIII">Seleciona o processador BIP III para ser utilizado na simulação.</system:String>

    <system:String x:Key="TipDescriptionRadioButtonBipIV">Seleciona o processador BIP IV para ser utilizado na simulação.</system:String>

    <system:String x:Key="TipDescriptionRadioButtonUBip">Seleciona o processador µBIP para ser utilizado na simulação.</system:String>

    <system:String x:Key="TipFooterTitle">Pressione F1 para obter mais ajuda.</system:String>

